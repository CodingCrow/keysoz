<div class="category category-{{extras.level}} js-comp js-category-component" id="{{extras.level}}" data-comp-path="seller/category_item/category" data-type="backCategory">
  <div class="category-header">
    <p class="item-name-p"><strong class="item-name">
      {{#equals extras.level 1}}一级类目{{/equals}}
      {{#equals extras.level 2}}二级类目{{/equals}}
      {{#equals extras.level 3}}三级类目{{/equals}}
      {{#equals extras.level 4}}四级类目{{/equals}}
      {{#equals extras.level 5}}产品SPU{{/equals}}
    </strong></p>
  </div>
  <div class="form category-body">
    <div class="search-item">
      <div class=" search-item-group">
        <i class="icon-mewtwo icon-mewtwo-search"></i>
        <input type="text" class="search-item-input js-search-item" placeholder="搜 索">
      </div>
    </div>
    <div class="top-li"></div>
    <ul class="divide-ul">
      {{#equals extras.level 5}}
        {{#each data}}
          <li class="divide-li" data-category="{{pp this}}">
            <div class="normal-status">
              {{#equals hasChildren "true"}}<i class="icon-mewtwo icon-mewtwo-right next"></i>{{/equals}}
              <a href="javascript:void(0);" class="item-pop"><span>{{name}}</span></a>
            </div>
          </li>
        {{/each}}
      {{else}}
        {{#each data}}
          <li class="divide-li" data-category="{{pp this}}">
            <div class="normal-status">
              {{#equals hasChildren "true"}}<i class="icon-mewtwo icon-mewtwo-right next"></i>{{/equals}}
              <a href="javascript:void(0);" class="item-pop"><span>{{name}}</span></a>
            </div>
          </li>
        {{/each}}
      {{/equals}}
    </ul>
  </div>
</div>

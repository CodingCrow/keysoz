<div class="form-section clearfix">
  <div class="section-title">
    普通属性
  </div>
  <div class="pull-left section-content">
    <div class="category-attribute attribute-area js-attribute-area">
      <ul class="attribute-list clearfix" id="js-category-attribute-list"
        {{#if workflowId}}
          {{#each _DATA_.changeValue.otherAttrs}}
            {{#equals group "普通属性"}}
              data-attr="{{json _DATA_.changeValue.otherAttrs.otherAttributeWithRules}}"
            {{/equals}}
          {{/each}}>
          {{#each _DATA_.changeValue.otherAttrs}}
            {{#of group attrGroup}}
              {{#each otherAttributeWithRules}}
                {{> component:seller/spu_detail/common/other_templates/_attribute_container}}
              {{/each}}
            {{/of}}
          {{/each}}
        {{else}}
          {{#equals group "普通属性"}}
            data-attr="{{json otherAttributeWithRules}}"
          {{/equals}}
          {{#each _DATA_.otherAttrs}}
            {{#of group attrGroup}}
              {{#each otherAttributeWithRules}}
                {{> component:seller/spu_detail/common/other_templates/_attribute_container}}
              {{/each}}
            {{/of}}
          {{/each}}
        {{/if}}
        
      </ul>
    </div>
  </div>
</div>
